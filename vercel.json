{
    "builds": [
      {
        "src": "PizzaPyWebApp/server/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "runtime": "python3.9"
        }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
            "distDir": "PizzaPyWebApp/static/staticfiles_build"
        }
      }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/PizzaPyWebApp/static/$1"
        },
      {
        "src": "/(.*)",
        "dest": "PizzaPyWebApp/server/wsgi.py"
      }
    ]
}
{
    "installCommand": "pip install -r requirements.txt && cd PizzaPyWebApp/js_lib && npm install",
    "builds": [
      {
        "src": "PizzaPyWebApp/server/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "runtime": "python3.9"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "PizzaPyWebApp/server/wsgi.py"
      }
    ]
}